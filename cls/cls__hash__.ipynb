    "class Display_hash_cls:\n",
    "    def __init__(self,name):\n",
    "        self.name = name\n",
    "    def __hash__(self):\n",
    "        return hash(self.name +'abc')\n",
    "b = Display_hash_cls('Jack')\n",
    "print(hash(b))"
